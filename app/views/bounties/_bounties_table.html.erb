<table class="table table-hover" id="bounties_table">
  <thead>
    <tr>
      <th>Bounty Issued by</th>
      <th>Bounty Price</th>
      <th>Bounty Issued</th>
      <th>Bounty Updated</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% issue.bounties.sort_by(&:price).reverse.each do |bounty| %>
      <%= render bounty %>
    <% end %>
  </tbody>
</table>
